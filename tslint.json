{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-any": { "severity": "error" },
    "no-non-null-assertion": true,
    "semicolon": [true, "always"],
    "no-default-export": true,
    "ordered-imports": {
      "severity": "default"
    }
  },
  "rulesDirectory": []
}
